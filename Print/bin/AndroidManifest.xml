            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.vjiazhi.shuiyinwang.ui.ImgMainActivity"
            android:label="@string/title_activity_img_main"
